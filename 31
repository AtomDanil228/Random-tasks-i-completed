#include <iostream>
using namespace std;
int main() {
    setlocale(LC_ALL, "rus");
    char row = 'A';
    cout << " ";
    for (int i = 0; i < 10; i++) {
        cout << static_cast<char>(i + 'A') << " ";
    }
    for (int i = 0; i < 10; i++) {
        cout << endl << row++ << " ";
        for (int j = 0; j < 10; j++) {
            cout << "0 ";
        }
    }
    cout << endl;
    return 0;
}
