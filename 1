#include <iostream>
using namespace std;
const int K = 7; const int N = 5;
void rectangle() {
    int massive[K][N];
    for (int i = 0; i < N; i++) {
        for (int j = 0; j < K; j++) {
            massive[i][j] = 0; cout << massive[i][j]; cout << " ";
        };
        cout << endl;
    };
};
int main()
{
    rectangle();
}
