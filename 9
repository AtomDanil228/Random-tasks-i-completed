#include <iostream>
const int a = 5;
int midNum(int mass[a]) {
    int j = 0; for (int i = 0; i < a; i++) {
        j = j + mass[i];
    } j = j / a; return j;
} using namespace std;
int main()
{
    setlocale(LC_ALL, "rus"); int massive[a]{ 1,4,5,12,50 };
    int num = midNum(massive);
    cout << "Среднее арифметическое массива = " << num << ".\n";
}
