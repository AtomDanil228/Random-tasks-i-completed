#include <iostream>
using namespace std;
int function(int n1, int n2)
{
    if (n1 == n2)
        return 1;
    else if (n1 > n2)
        return 2;
    else if (n2 > n1)
        return 3;
}
int main()
{
    setlocale(LC_ALL, "rus");
    int number1 = 75, number2 = 10; int func = function(number1, number2);
    if (func == 1)
        cout << "Числа " << number1 << " и " << number2 << "Равны друг другу";
    else if (func == 2)
        cout << number1;
    else if (func == 3)
        cout << number2;
}
