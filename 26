#include <iostream>
using namespace std;
int main() {
    setlocale(LC_ALL, "rus");
    int a;
    cout << "Введите число a (1 <= a <= 20): ";
    cin >> a;

    int product = 1;
    for (int i = a; i <= 20; ++i) {
        product *= i;
    }

    cout << "Произведение всех целых чисел от " << a << " до 20 равно " << product << endl;
    return 0;
}
