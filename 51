using System;

namespace ConsoleApp1
{
    class Program
    {
        static void Main(string[] args)
        {
            int[] myArray = { 1, 3, 5, 7, 9, 11, 17, 19, 23 };
            int search = 7;
            int index = Array.BinarySearch(myArray, search);
            if(index >= 0)
            {
                Console.WriteLine("Index = " + index);
            }
            else
            {
                Console.WriteLine("Index not found");
            }
        }
    }
}
