#include <iostream>
using namespace std;
int function(int n)
{
    if (n >= 0)
        return true;
    else
        return false;
}
int main()
{
    setlocale(LC_ALL, "rus");
    int number = 75; int func = function(number);
    if (func == false)
        cout << "False";
    else
        cout << "True";
}
