using System;
using System.Collections.Generic;
namespace ConsoleApp1
{
    class Program
    {
        static void Main(string[] args)
        {
            int[] arr = { 10, 20, 40, 50, 30 };
            int search = 30;
            int index = Array.IndexOf(arr, search);
            if(index >= 0)
            {
                Console.WriteLine("Index = " + index);
            }
            else
            {
                Console.WriteLine("Index not found");
            }
        }
    }
}
